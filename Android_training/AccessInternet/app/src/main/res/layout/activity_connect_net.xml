<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.li.ramon.accessinternet.connect.ConnectNetActivity">
    <Button
        android:id="@+id/get_info"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/get_wed_info"
        android:onClick="myClickHandler"/>
    <EditText
        android:id="@+id/edit_url"
        android:layout_alignParentLeft="true"
        android:layout_toLeftOf="@id/get_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/get_info"/>

    <TextView
        android:layout_below="@id/edit_url"
        android:id="@+id/show_web_info"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorGray"/>

</RelativeLayout>
