<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ramon.better.top.androidui.testactivity.PropertiesPriorityViewActivity">
    <!-- StyleTestView 验证属性的优先级
    直接在XML中定义 > style定义 > 由defStyleAttr定义的值 > defStyleRes指定的默认值 > 直接在Theme中指定的值 -->
    <ramon.better.top.androidui.customview.PropertiesPriorityView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:attr_one="attr one in xml"
        style="@style/ThroughStyle"
        android:background="@color/blue"/>
</android.support.constraint.ConstraintLayout>
